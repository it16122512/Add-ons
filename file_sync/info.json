{
  "name": "File Sync SSL",
  "slug": "file_sync",
  "description": "Sync SSL certificates from Nginx Proxy Manager to Asterisk inside Home Assistant OS",
  "version": "1.0.0",
  "url": "https://github.com/it16122512/Add-ons/tree/main/file_sync",
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "startup": "application",
  "boot": "auto",
  "hassio_api": true,
  "host_network": false,
  "init": false,
  "map": [
    "addon_configs:ro",
    "ssl:rw"
  ],
  "options": {
    "source_relative_path": "a0d7b954_nginxproxymanager/letsencrypt/live/npm-8",
    "dest_relative_path": "nginxproxymanager/live/npm-1"
  },
  "schema": {
    "source_relative_path": "str",
    "dest_relative_path": "str"
  }
}
